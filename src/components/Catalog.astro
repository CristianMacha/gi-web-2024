---
import Image from "astro/components/Image.astro";
interface Props {
    title: string;
    image: string;
}
const { title } = Astro.props;
const { image } = Astro.props;
---

<div class="flex flex-col ">
    <Image
        src={image}
        alt={title}
        width={200}
        height={250}
        class={'rounded-2xl w-[250px] h-[250px]'}
    />
    <!-- <h3 class="text-[#00000080] text-sm">{title}</h3>   -->
</div>
